{
    "name": "next-cms",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "lang:generate": "bash generateLangFolders.sh",
        "lang:clean": "bash cleanLangFolders.sh",
        "prisma:dev:migrate": "prisma migrate dev --skip-seed",
        "prisma:seed": "prisma db seed",
        "prisma:generate": "prisma generate",
        "prisma:prod:migrate": "prisma migrate deploy",
        "prisma:push": "prisma db push --accept-data-loss",
        "prisma:studio": "prisma studio",
        "prisma:dev": "npm run prisma:dev:migrate && npm run prisma:seed",
        "prisma:prod": "npm run prisma:generate && npm run prisma:prod:migrate && npm run prisma:push && npm run prisma:seed",
        "lint": "next lint",
        "test": "playwright test",
        "test:head": "playwright test --headed",
        "analyze": "ANALYZE=true npm run build",
        "--------------------------------------------": "--------------------------------------------",
        "prebuild": "npm run lang:generate && npm run prisma:prod",
        "build": "next build",
        "start": "next start",
        "predev": "npm run lang:generate && npm run prisma:dev",
        "dev": "next dev -p 8080"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
    "dependencies": {
        "@formkit/auto-animate": "^0.7.0",
        "@prisma/client": "^5.0.0",
        "@tanstack/react-query": "^4.24.10",
        "@tanstack/react-query-devtools": "^4.24.12",
        "antd": "^5.7.0",
        "axios": "^1.3.4",
        "bcryptjs": "^2.4.3",
        "classnames": "^2.3.2",
        "dayjs": "^1.11.7",
        "draft-convert": "^2.1.13",
        "draft-js": "^0.11.7",
        "eslint": "^8.43.0",
        "eslint-config-next": "^13.4.7",
        "formidable": "^3.2.5",
        "formik": "^2.2.9",
        "lodash.difference": "^4.5.0",
        "lodash.get": "^4.4.2",
        "lodash.set": "^4.3.2",
        "mime-types": "^2.1.35",
        "mv": "^2.1.1",
        "next": "^13.2.4",
        "prettier": "^3.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recharts": "^2.7.2"
    },
    "devDependencies": {
        "@playwright/test": "^1.30.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/draft-convert": "^2.1.4",
        "@types/draft-js": "^0.11.10",
        "@types/formidable": "^2.0.5",
        "@types/lodash.get": "^4.4.7",
        "@types/lodash.set": "^4.3.7",
        "@types/mime-types": "^2.1.1",
        "@types/mv": "^2.1.2",
        "@types/node": "^20.3.2",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "eslint-config-prettier": "^8.5.0",
        "prettier-plugin-prisma": "^5.0.0",
        "prisma": "^5.0.0",
        "sass": "^1.58.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}
