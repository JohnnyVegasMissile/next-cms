{
    "name": "next-cms",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 8080",
        "prebuild": "prisma migrate deploy; npx prisma db push; npx prisma db seed",
        "build": "next build",
        "postbuild": "next-sitemap",
        "start": "next start",
        "lint": "next lint",
        "initdb": "prisma migrate dev; npx prisma db seed",
