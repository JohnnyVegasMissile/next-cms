{
    "name": "next-cms",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prisma:dev:migrate": "prisma migrate dev --skip-seed",
        "prisma:seed": "prisma db seed",
        "prisma:genarate": "prisma generate",
        "prisma:prod:migrate": "prisma migrate deploy",
        "prisma:push": "prisma db push --accept-data-loss",
        "prisma:studio": "prisma studio",
        "prisma:dev": "npm run prisma:dev:migrate && npm run prisma:seed",
        "prisma:prod": "npm run prisma:genarate && npm run prisma:prod:migrate && npm run prisma:push && npm run prisma:seed",
        "lint": "next lint",
        "test": "playwright test",
        "test:head": "playwright test --headed",
        "analyze": "ANALYZE=true npm run build",
        "": "--------------------------------------------",
        "prebuild": "npm run prisma:prod",
        "build": "next build",
        "start": "next start",
        "predev": "npm run prisma:dev",
        "dev": "next dev -p 8080"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
    "dependencies": {
        "@prisma/client": "^4.10.1",
        "@tanstack/react-query": "^4.24.10",
        "@tanstack/react-query-devtools": "^4.24.12",
        "@types/node": "18.14.0",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "antd": "^5.2.1",
        "axios": "^1.3.4",
        "bcryptjs": "^2.4.3",
        "classnames": "^2.3.2",
        "dayjs": "^1.11.7",
        "eslint": "8.34.0",
        "eslint-config-next": "^13.1.6",
        "formidable": "^2.1.1",
        "formik": "^2.2.9",
        "lodash.get": "^4.4.2",
        "lodash.set": "^4.3.2",
        "mime-types": "^2.1.35",
        "mv": "^2.1.1",
        "next": "^13.1.7-canary.21",
        "react": "^18.2.0",
        "react-colorful": "^5.6.1",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@playwright/test": "^1.30.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/formidable": "^2.0.5",
        "@types/lodash.get": "^4.4.7",
        "@types/lodash.set": "^4.3.7",
        "@types/mime-types": "^2.1.1",
        "@types/mv": "^2.1.2",
        "eslint": "8.18.0",
        "eslint-config-next": "12.1.6",
        "eslint-config-prettier": "^8.5.0",
        "prettier": "2.7.1",
        "prettier-plugin-prisma": "^4.4.0",
        "prisma": "^4.10.1",
        "sass": "^1.58.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}
